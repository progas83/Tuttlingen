<Window x:Class="CalcPluginUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalcPluginUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <Label Content="x1: "/>
            <TextBox x:Name="x1UI" Width="100"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="x2: "/>
            <TextBox x:Name="x2UI" Width="100"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="result: "/>
            <TextBox x:Name="resultUI" Width="100"/>
        </StackPanel>

        <Button Margin="50" Width="50" Height="25" Content="PUSH" Click="Button_Click"/>

        <Button Margin="50" Width="50" Height="25" Content="PUSH`test" Click="Button1_Click"/>
    </StackPanel>
</Window>
