    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="DataRow">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PackageId" type="xs:int" minOccurs="0" maxOccurs="1" />
              <xs:element name="PackageSSCC" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="OrderNo" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="TrackingNo" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="ShipmentDate" type="xs:date" minOccurs="0" maxOccurs="1" />
              <xs:element name="ShipmentSystem" type="xs:string" minOccurs="0" maxOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>